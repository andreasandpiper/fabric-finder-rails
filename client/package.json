{
  "name": "react_starter",
  "version": "1.0.0",
  "description": "Starting setup for react redux project",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "bundle": "node_modules/.bin/webpack --config webpack.deploy.config.js --progress --colors",
    "test": "react-scripts test --env=jsdom",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "build": "react-scripts build",
    "watch": "webpack --config webpack.config.js --watch"
  },
  "author": "Andrea Wayte",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@github.com:andreasandpiper/fabric-finder-rails.git"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "0.0.18",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.6.2",
    "node-sass-chokidar": "^1.2.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.3.1",
    "postcss-loader": "^2.0.6",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  }
}
